{
    "name": "mindshaker/image-upload",
    "description": "Laravel Image Upload",
    "license": "Apache-2.0",
    "keywords": [
        "laravel",
        "image",
        "upload"
    ],
    "authors": [
        {
            "name": "Miguel Neves",
            "email": "miguel.neves@mindshaker.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "intervention/image": "^3.4"
    },
    "require-dev": {
        "laravel/framework": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Mindshaker\\ImageUpload\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mindshaker\\ImageUpload\\ImageUploadServiceProvider"
            ],
            "aliases": {
                "ImageUpload": "Mindshaker\\ImageUpload\\Facades\\ImageUpload"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}